{
  "kind": "collectionType",
  "collectionName": "apartments",
  "info": {
    "singularName": "apartment",
    "pluralName": "apartments",
    "displayName": "Nekretnina",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Naziv": {
      "type": "string",
      "required": true
    },
    "Opis": {
      "type": "richtext",
      "required": true
    },
    "Slike": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Tip": {
      "type": "enumeration",
      "enum": [
        "Stan",
        "Kuca"
      ],
      "required": true
    },
    "Detalji": {
      "displayName": "apartment-details",
      "type": "component",
      "repeatable": false,
      "component": "apartment-details.apartment-details",
      "required": true
    },
    "Prodato": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "Grad": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::city.city",
      "inversedBy": "Nekretnine"
    },
    "Istaknuto": {
      "type": "boolean",
      "default": false
    },
    "Latitude": {
      "type": "float",
      "required": true
    },
    "Longitude": {
      "type": "float",
      "required": true
    }
  }
}
